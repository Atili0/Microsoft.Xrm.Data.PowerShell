        <CommandParameterSetInfo2 Name="CrmRecord" Params="conn CrmRecord ActivityEntityType Subject Description OnwerUserId Fields Verbose Debug ErrorAction WarningAction InformationAction ErrorVariable WarningVariable InformationVariable OutVariable OutBuffer PipelineVariable" />
